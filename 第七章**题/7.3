import java.util.Scanner;  
import java.util.Arrays;//排序头文件  
//7.3   
  
public class Main {  
    public static void main(String[] args) {  
        System.out.println("Enter the integer between 1 and 100:");  
        Scanner str = new Scanner(System.in);  
        int x = str.nextInt();  
        int[] a = new int[101];  
        int i = 0;  
        a[0] = x;  
        while (x != 0) {// 输入0结束  
            i++;  
            x = str.nextInt();  
            a[i] = x;  
        }  
        int j = i;  
        for (i = 0; i < j; i++) {  
            System.out.print(a[i] + " ");  
        }  
        System.out.println();  
  
        Arrays.sort(a, 0, j);// 排序,a[],从0~j  
        for (i = 0; i < j; i++) {  
            System.out.print(a[i] + " ");  
        }  
        System.out.println();  
  
        int[] b = new int[j];  
        int[] c = new int[j];  
        b[0] = 1;  
        c[0] = a[0];  
        int k;  
        for (i = 1, k = 0; i < j; i++) {  
            if (a[i] != a[i - 1]) {  
                c[k] = a[i];  
                k++;  
            }  
        }  
        for (i = 0; i <= k; i++) {  
            System.out.print(c[i] + " ");  
        }  
  
    }  
}
